<?xml version="1.0" encoding="UTF-8"?>
<OpenSimDocument Version="20302">
	<IKTool name="TrialName_IK">

    <!-- ********************************************************************** -->
    <!--      Input & Solver Specifications                                     -->
    <!-- ********************************************************************** -->

    <!--Directory used for writing results.-->
		<results_directory> ./ </results_directory>
		<!--Directory for input files-->
		<input_directory>  </input_directory>
		<!--Name of the .osim file used to construct a model.-->
		<model_file> subjectID__Simbody.osim </model_file>
		<!--A positive scalar that is used to weight the importance of satisfying
		    constraints. A weighting of 'Infinity' or if it is unassigned results
		    in the constraints being strictly enforced.-->
		<constraint_weight> 20.00000000 </constraint_weight>
		<!--The accuracy of the solution in absolute terms. (i.e. the number of
		    significant digits to which the solution can be trusted.)-->
		<accuracy> 0.00001000 </accuracy>

    <!-- ********************************************************************** -->
    <!--      Inverse Kinematics Tasks                                          -->
    <!-- ********************************************************************** -->

    <!--Markers and coordinates to be considered (tasks) and their weightings.-->
		<IKTaskSet name="IK_Tasks">
			<objects>

        <!-- 
        { IKMarkerTask Object } 
          apply - whether or not this task will be used during the inverse kinematics solve
          weight - weight given to a marker or coordinate for solving inverse kinematics 
        -->
        
        <IKMarkerTask name="R.ACROMION">
          <apply> true </apply>
          <weight> 10.00000000 </weight>
        </IKMarkerTask>
        <IKMarkerTask name="L.ACROMION">
          <apply> true </apply>
          <weight> 10.00000000 </weight>

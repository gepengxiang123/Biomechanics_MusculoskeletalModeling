					<weight> 10.00000000 </weight>
				</IKMarkerTask>
				<IKMarkerTask name="V.SACRAL">
					<apply> true </apply>
					<weight> 10.00000000 </weight>
				</IKMarkerTask>
        <IKMarkerTask name="R.HIP.POSTERIOR">
          <apply> true </apply>
          <weight> 1.00000000 </weight>
        </IKMarkerTask>
        <IKMarkerTask name="L.HIP.POSTERIOR">
